<form method="post" action="{{ path('motorcycle_delete', {'id': motorcycle.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ motorcycle.id) }}">
	<button class="btn">
		<i class="bi bi-archive-fill icon-delete"></i>
	</button>
</form>
