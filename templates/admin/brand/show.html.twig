{% extends 'base_admin.html.twig' %}

{% block title %}Brand
{% endblock %}

{% block body %}
	<h1>Brand</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ brand.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ brand.name }}</td>
			</tr>
			<tr>
				<th>Logo</th>
				<td>{{ brand.logo }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ brand.createdAt ? brand.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ brand.updatedAt ? brand.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('admin_brand_index') }}">back to list</a>

	<a href="{{ path('admin_brand_edit', {'id': brand.id}) }}">edit</a>

	{{ include('admin/brand/_delete_form.html.twig') }}
{% endblock %}
